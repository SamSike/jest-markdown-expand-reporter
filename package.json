{
	"name": "jest-markdown-expandable-reporter",
	"version": "0.0.1",
	"description": "An expandable markdown reporter for Jest tests",
	"keywords": [],
	"license": "MIT",
	"original-author": "Chris Shepherd",
	"original-author-url": "https://www.npmjs.com/package/jest-gh-md-reporter",
	"author": "Sameer Sikka",
	"main": "src/mdReporter.js",
	"scripts": {
		"test:mutation": "stryker run",
		"test:unit": "rm -rf .stryker-tmp && jest --coverage --testPathIgnorePatterns=mdReporter.test.js",
		"test": "jest mdReporter.test.js"
	},
	"jest": {
		"testEnvironment": "./src/logEnv.js",
		"reporters": [
			[
				"./src/mdReporter",
				{
					"filename": "test-report.md",
					"publicPath": "./test-reports",
					"detailed": true,
					"displayAllTests": true
				}
			]
		],
		"testMatch": [
			"**/__tests__/*.test.js"
		]
	},
	"dependencies": {
		"ejs": "^3.1.10"
	},
	"devDependencies": {
		"@stryker-mutator/core": "^9.0.1",
		"@stryker-mutator/jest-runner": "^9.0.1",
		"jest": "^29.7.0",
		"jest-environment-node": "^30.0.0-beta.3",
		"jest-when": "^3.7.0",
		"prettier": "^3.5.3"
	}
}
